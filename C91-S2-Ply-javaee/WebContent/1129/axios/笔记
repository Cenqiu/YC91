// 异步调用
// 回调地狱
jQuery.get( url 地址, params 参数, function(data){
	//...省略...
	jQuery.get( url 地址, params 参数, function(data){
		//...省略...
		jQuery.get( url 地址, params 参数, function(data){
			//...省略...
		})
	})
})

